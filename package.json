{
    "name": "AppStructure",
    "version": "1.0.0",
    "buildNumber": "1",
    "private": true,
    "scripts": {
        "android": "npx react-native run-android --verbose",
        "ios": "npx react-native run-ios",
        "iosKnownDevices": "xcrun instruments -s devices",
        "start": "npx react-native start",
        "clean": "yarn run cleanBuildAndroid && yarn run cleanBuildIOS",
        "cleanBuildAndroid": "rm -rf android/app/build && rm -rf android/build && cd android && ./gradlew clean && cd ..",
        "cleanBuildIOS": "rm -rf ios/build",
        "debugApk": "yarn run clean && cd android && ./gradlew assembleDebug && cd ..",
        "releaseApk": "yarn run clean && cd android && ./gradlew assembleRelease && cd ..",
        "test": "jest",
        "postinstall": "npx patch-package",
        "updateVersion": "node node_modules/react-native-version-env-manager --updateVersion",
        "env:local": "node node_modules/react-native-version-env-manager --env=local --envFileExt=ts --addEnvAsPostfix",
        "env:dev": "node node_modules/react-native-version-env-manager --env=dev --envFileExt=ts --addEnvAsPostfix",
        "env:qa": "node node_modules/react-native-version-env-manager --env=qa --envFileExt=ts --addEnvAsPostfix",
        "env:stag": "node node_modules/react-native-version-env-manager --env=stag --envTag=staging --envFileExt=ts --addEnvAsPostfix",
        "env:prod": "node node_modules/react-native-version-env-manager --env=prod --envFileExt=ts",
        "prepare": "husky install",
        "lint": "eslint ."
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "^1.15.7",
        "@react-native-community/masked-view": "^0.1.10",
        "@react-native-segmented-control/segmented-control": "^2.4.0",
        "@react-navigation/native": "^6.0.2",
        "@react-navigation/stack": "^6.0.7",
        "@reduxjs/toolkit": "^1.8.1",
        "@types/react": "^17",
        "@types/react-native-linear-gradient": "^2.4.0",
        "apisauce": "^2.0.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "formik": "^2.1.5",
        "global": "^4.4.0",
        "husky": "^7.0.4",
        "metro-babel-register": "^0.70.3",
        "moment": "^2.29.1",
        "prettier": "^2.6.1",
        "pretty-ms": "^7.0.1",
        "react": "18.1.0",
        "react-native": "0.70.6",
        "react-native-check-box": "^2.1.7",
        "react-native-cli": "^2.0.1",
        "react-native-elements": "^3.2.0",
        "react-native-gesture-handler": "1.10.3",
        "react-native-keychain": "^7.0.0",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-onesignal": "^4.3.0",
        "react-native-pager-view": "^5.4.1",
        "react-native-permissions": "^3.0.5",
        "react-native-progress": "^5.0.0",
        "react-native-reanimated": "^2.2.2",
        "react-native-safe-area-context": "^3.0.7",
        "react-native-screens": "^3.6.0",
        "react-native-simple-toast": "^1.1.2",
        "react-native-svg": "^12.1.0",
        "react-native-tab-view": "^3.0.1",
        "react-native-vector-icons": "^8.0.0",
        "react-native-version-check": "^3.4.2",
        "react-native-webview": "^11.6.5",
        "react-navigation": "latest",
        "react-redux": "^8.0.1",
        "reanimated-bottom-sheet": "^1.0.0-alpha.22",
        "rn-fetch-blob": "^0.12.0",
        "toggle-switch-react-native": "^3.2.0",
        "yup": "^0.32.8"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@expo/metro-config": "^0.1.56",
        "@react-native-community/eslint-config": "^2.0.0",
        "@testing-library/jest-native": "^4.0.2",
        "@testing-library/react-native": "^7.1.0",
        "@types/jest": "^27.0.1",
        "@types/react-native": "^0.70.7",
        "@types/react-native-check-box": "^2.1.2",
        "@types/react-test-renderer": "^18.0.0",
        "@types/rn-fetch-blob": "^1.2.1",
        "@types/yup": "^0.29.6",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "babel-jest": "^26.6.3",
        "eslint": "^7.32.0",
        "jest": "^26.6.3",
        "lint-staged": "^11.1.2",
        "metro-react-native-babel-preset": "^0.67.0",
        "react-native-codegen": "^0.0.7",
        "react-native-svg-transformer": "^0.14.3",
        "react-native-version-env-manager": "^1.2.4",
        "react-test-renderer": "18.2.0",
        "ts-jest": "^26.5.1",
        "typescript": "^4.0.2"
    },
    "resolutions": {
        "@types/react": "^17"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    }
}
